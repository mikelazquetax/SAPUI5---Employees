<core:FragmentDefinition xmlns:core = "sap.ui.core" xmlns="sap.m">
    <Dialog id="_IDGenDialog1" title="{i18n>orders}" >

        <Table id="_IDGenTable1" items="{odataNorthwind>Orders}" width="700px">
            <columns>
                <Column id="_IDGenColumn1" >
                <header>
                    <Label id="_IDGenLabel1" text="{i18n>orderID}"/>
                </header>
                </Column>
                <Column id="_IDGenColumn2" >
                <header>
                    <Label id="_IDGenLabel2" text="{i18n>Freight}"/>
                </header>
                </Column>
                <Column id="_IDGenColumn3" >
                <header>
                    <Label id="_IDGenLabel3" text="{i18n>ShipAddress}"/>
                </header>
                </Column>
            </columns>
            <items>
                <ColumnListItem id="_IDGenColumnListItem1" >
                <cells>
                    <Label id="_IDGenLabel4" text="{odataNorthwind>OrderID}"></Label>
                    <Label id="_IDGenLabel5" text="{odataNorthwind>Freight}"></Label>
                    <Label id="_IDGenLabel6" text="{odataNorthwind>ShipAddress}"></Label>
                </cells>
                </ColumnListItem>
            </items>
        </Table>
        <endButton>
            <Button id="_IDGenButton1" text="{i18n>close}" press="cerrarModal"></Button>
        </endButton>
    </Dialog>
</core:FragmentDefinition>